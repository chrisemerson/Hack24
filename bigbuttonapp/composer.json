{
    "name": "bigbutton/bigbutton",
    "config": {
      "bin-dir": "bin"
    },
    "require": {
        "guzzlehttp/guzzle": "~5.2",
        "esendex/sdk": "^1.0",
        "vivait/chronofy-php": "dev-master@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.2",
        "behat/behat": "~3.0"
    },
    "authors": [
      {
        "name": "Chris Emerson"
      },
      {
        "name": "Richard Bagshaw"
      },
      {
        "name": "Lewis Wright"
      },
      {
        "name": "James Fowkes"
      }
    ],
    "autoload": {
        "psr-4": {
            "BigButton\\App\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\BigButton\\App\\": ["spec/"]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LewisW/chronofy-php"
        }
    ]
}
